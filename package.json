{
  "name": "@dhis2/ui",
  "version": "1.0.0-beta.9",
  "main": "index.js",
  "module": "index.mjs",
  "sideEffects": [
    "*.css"
  ],
  "repository": "git@github.com:dhis2/ui.git",
  "author": "Viktor Varland <viktor@dhis2.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build/*",
    "format": "packages code-style",
    "release": "packages release",
    "size": "size-limit",
    "start": "node scripts/watch.js",
    "sort:css": "node scripts/sort-css.js",
    "build:css": "postcss --verbose 'src/**/*.css' --base src --dir build",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir build --copy-files --verbose",
    "build:modules": "BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.mjs --verbose",
    "build:docs": "node scripts/docs.js",
    "build": "NODE_ENV=production npm run clean && npm run build:cjs && npm run build:modules && npm run build:css"
  },
  "husky": {
    "hooks": {
      "commit-msg": "packages commit-style",
      "pre-commit": "packages code-style"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@dhis2/packages": "^1.3.0",
    "chokidar": "^2.0.4",
    "conventional-changelog-cli": "^2.0.5",
    "css-declaration-sorter": "^4.0.1",
    "cssnano": "^4.1.3",
    "fs-extra": "^7.0.0",
    "husky": "^1.0.1",
    "postcss-cli": "^6.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-inline-rtl": "^0.9.8",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.0.6",
    "react-dev-utils": "^7.0.3",
    "size-limit": "^0.21.1",
    "string-hash": "^1.1.3"
  },
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "material-design-icons-iconfont": "^4.0.2",
    "typeface-roboto": "^0.0.54"
  }
}
