{
  "name": "@dhis2/ui-core",
  "version": "0.1.0",
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "sideEffects": false,
  "repository": "git@github.com:dhis2/ui.git",
  "author": "Viktor Varland <viktor@dhis2.org>",
  "license": "BSD-3-Clause",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./build/*",
    "size": "size-limit",
    "start": "node scripts/watch.js",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir ./build/cjs --copy-files --verbose",
    "build:modules": "BABEL_ENV=modules babel src --out-dir ./build/es --copy-files --verbose",
    "build:docs": "cd ./docs && yarn install && CI=false yarn build && cd .. && cp -r ./docs/build ./build/docs",
    "build": "NODE_ENV=production yarn clean && yarn build:commonjs && yarn build:modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@dhis2/cli-style": "^2.2.1",
    "@dhis2/packages": "^1.3.0",
    "chokidar": "^2.0.4",
    "conventional-changelog-cli": "^2.0.5",
    "fs-extra": "^7.0.0",
    "husky": "^1.0.1",
    "react-dev-utils": "^8.0.0",
    "size-limit": "^0.21.1"
  },
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16.3",
    "react-dom": "^16.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "styled-jsx": "^3.2.1"
  },
  "files": [
    "./build"
  ]
}
