{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.cjs.js",
  "module": "index.js",
  "browser": "index.umd.js",
  "sideEffects": [
    "*.css"
  ],
  "repository": "git@github.com:dhis2/ui.git",
  "author": "Viktor Varland <viktor@dhis2.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test tape -r babel-register tests/*.js",
    "stats": "webpack --profile --progress --json > bundle.stats.json && webpack-bundle-analyzer bundle.stats.json",
    "start": "node scripts/watch.js",
    "build:css": "postcss --verbose src/**/*.css --base src --dir build",
    "build:es": "babel src --out-dir build --copy-files",
    "build:files": "node scripts/setup-dist-package.js && node scripts/copy-files.js",
    "build": "NODE_ENV=production npm run build:es && npm run build:css && npm run build:files",
    "deploy": "deploy-build"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@dhis2/deploy-build": "^1.0.4",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "chokidar": "^2.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "eslint": "^5.4.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^2.0.0",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.4.1",
    "nodemon": "^1.18.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-cli": "^6.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-rtl": "^1.3.2",
    "prop-types-exact": "^1.2.0",
    "react-dev-utils": "^5.0.1",
    "style-loader": "^0.23.0",
    "tape": "^4.9.1",
    "typeface-rubik": "^0.0.54",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "peerDependencies": {
    "@babel/polyfill": "^7.0.0-rc.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  }
}
