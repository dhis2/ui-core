{
  "name": "@dhis2/ui",
  "version": "1.0.0-beta.14",
  "main": "index.js",
  "module": "index.es.js",
  "sideEffects": false,
  "repository": "git@github.com:dhis2/ui.git",
  "author": "Viktor Varland <viktor@dhis2.org>",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build/*",
    "release": "packages release",
    "size": "size-limit",
    "start": "node scripts/watch.js",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir build --copy-files --verbose",
    "build:modules": "BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js --verbose",
    "build:docs": "node scripts/docs.js",
    "build": "NODE_ENV=production npm run clean && npm run build:cjs && npm run build:modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@dhis2/cli-style": "^2.2.1",
    "@dhis2/packages": "^1.3.0",
    "chokidar": "^2.0.4",
    "conventional-changelog-cli": "^2.0.5",
    "fs-extra": "^7.0.0",
    "husky": "^1.0.1",
    "react-dev-utils": "^8.0.0",
    "size-limit": "^0.21.1",
    "string-hash": "^1.1.3"
  },
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "styled-jsx": "^3.2.1"
  }
}
